    std_right base
    center base
;
--- T
clip left=std_left-1 right=std_right+1 top=uppercase_top-1 bottom=base+1;
stroke vert center;
stroke horz uppercase_top;
--- Z
clip left=std_left-1 right=std_right+1 top=uppercase_top-1 bottom=base+1;
stroke horz uppercase_top;
stroke horz base;
stroke diag std_left base-1 std_right uppercase_top+1;
